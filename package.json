{
  "name": "arttaca-backend",
  "version": "1.0.0",
  "description": "",
  "author": "Dandelion Labs (hello@dandelionlabs.io)",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compile-contracts": "typechain --target ethers-v5 --out-dir src/contracts \"./src/contracts/*.json\""
  },
  "dependencies": {
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/wallet": "^5.7.0",
    "@googleapis/docs": "^2.0.1",
    "@logtail/node": "^0.2.2",
    "@logtail/winston": "^0.2.2",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.3.5",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.5",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.2",
    "@nestjs/platform-express": "^9.3.5",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.2.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/nodemailer": "^6.4.7",
    "@types/uuid": "^9.0.0",
    "alchemy-sdk": "^2.6.1",
    "aws-sdk": "^2.1322.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.1.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ethers": "^5.5.3",
    "googleapis": "^117.0.0",
    "mongoose": "^6.9.1",
    "nanoid": "3.3.4",
    "nest-winston": "^1.8.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.1",
    "oauth": "^0.10.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^8.0.0-alpha.7",
    "simple-node-logger": "^21.8.12",
    "swagger-ui-express": "^4.6.0",
    "typechain": "^8.1.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.5",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
